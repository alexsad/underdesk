#end
#end

	public $classe.getNome()(){
		super();
	}
	
#foreach ($campo in $classe.getAllCampos())	
	public $campo.getTipo() get$campo.getColunaCapitalize()(){
		return this.$campo.getColuna();
	}
	public void set$campo.getColunaCapitalize()($campo.getTipo() $campo.getColuna()){
		this.$campo.getColuna() = $campo.getColuna();
	}	
#end
}
