#if($classe.getTipo()=="table")
@Entity
@Table(name="${classe.getNomeLogical()}")
#elseif($classe.getTipo()=="view")	
@Entity(name="${classe.getNomeLogical()}")
@Immutable
@Subselect("SELECT "+
